{
  "manifest_version": 3,
  "name": "Kalinda AI",
  "version": "1.0",
  "description": "A Chrome extension powered by GPT to help with my Quiz.",
  "permissions": [
    "clipboardRead",
    "activeTab",
    "storage"
  ],
  "host_permissions": [
    "http://localhost:3000/"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; connect-src http://localhost:3000/"
  }
}